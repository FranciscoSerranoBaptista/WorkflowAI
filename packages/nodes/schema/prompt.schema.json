{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "temperature": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "maxTokens": {
      "type": "integer",
      "minimum": 1
    },
    "content": {
      "type": "string"
    }
  },
  "required": ["id", "description", "content"],
  "additionalProperties": true
}
